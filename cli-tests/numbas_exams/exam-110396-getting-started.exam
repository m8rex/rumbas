          "functions": {},
          "preamble": { "js": "", "css": "" },
          "parts": [
            {
              "type": "information",
              "useCustomName": true,
              "customName": "Exam navigation modes",
              "marks": 0,
              "scripts": {},
              "customMarkingAlgorithm": "",
              "extendBaseMarkingAlgorithm": true,
              "unitTests": [],
              "showCorrectAnswer": true,
              "showFeedbackIcon": true,
